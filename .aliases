# Aliases called by either Zsh or Bash dotfiles

# Load files in Atom from command line more quickly:
alias a="atom"


# Git
## Git add all, then commit
alias gac="git add -A && git commit -m"



# Directory Jumps
## Switch to projects
alias proj="$HOME/Projects"


# Quick App Opens

alias dige="open ~/Projects/dot_files/Dashboard/database_digital_eurasia_6.1.fmp12"
alias pahdic="open ~/Projects/dot_files/Dashboard/pahlavi_dictionary.fmp12"

alias jyp="jupyter notebook --notebook-dir='~/Projects/jupyter_notebooks'"
